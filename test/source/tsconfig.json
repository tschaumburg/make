{
	"compilerOptions": {
		"sourceRoot": ".",
		"outDir": "../build",
		"noResolve": false,
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"removeComments": false,
		"sourceMap": true,
		"inlineSources": true,
		"noEmitOnError": true,
		"noLib": false,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"target": "es6",
		"lib": [
			"es6"
		]
	},
	"exclude": [
		"build"
	],
	"include": [
		"."
	]
}
